    {
    if (-0.2 &lt;= hspeed) and (hspeed &lt;= 0.2)
        {
        //show_message("ligma")
        if hspeed &gt;= 0
            {hspeed -= 0.01}
        else if hspeed &lt;= 0
            {hspeed += 0.01}
        }
    
    else
        {
        if hspeed &gt;= 0
            {hspeed -= 0.2}
        else if hspeed &lt;= 0
            {hspeed += 0.2}
        }
    }
    
    
//Speed Cap
if vspeed != 0
    {
    if vspeed &gt; SpeedCap
        {vspeed = SpeedCap}
    else if vspeed &lt; -SpeedCap
        {vspeed = -SpeedCap}
    }
    
if hspeed != 0
    {
    if hspeed &gt; SpeedCap
        {hspeed = SpeedCap}
    else if hspeed &lt; -SpeedCap
        {hspeed = -SpeedCap}
    }
